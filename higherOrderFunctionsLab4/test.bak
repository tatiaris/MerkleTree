; load the files
(load "sort.scm")
(load "apps.scm")

(display (map + '(1 2 3) '(4 5 6)))
;square each item of '(1 2 3)
(display (map (lambda (x) (* x x)) '(1 2 3)))

;function curve that will accept a list and add 10% to the grades

(display (map (lambda (x) (* 1.1  x)) '(80 90 100)))

; the filter function for positive values
(display (filter positive? '(1 -3 4 7 -4)))

;testing other filter functions
(println)
(display (filter negative? '(1 -3 4 7 -4)))
(display (filter zero? '(0 1 -3 4 7 -4)))

(display (filter even? '(0 1 -3 4 7 -4 )))


